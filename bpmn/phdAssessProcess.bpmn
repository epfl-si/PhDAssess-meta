           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html3&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Doctoral candidate comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;phdComment2&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisDirectorComment2&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisCoDirectorComment2&#34;,           &#34;logic&#34;: [             {               &#34;name&#34;: &#34;Comment1CoDirectorLogic&#34;,               &#34;trigger&#34;: {                 &#34;type&#34;: &#34;simple&#34;,                 &#34;simple&#34;: {                   &#34;show&#34;: true,                   &#34;when&#34;: &#34;thesisCoDirectorName&#34;,                   &#34;eq&#34;: &#34;&#34;                 }               },               &#34;actions&#34;: [                 {                   &#34;name&#34;: &#34;Comment1Action&#34;,                   &#34;type&#34;: &#34;property&#34;,                   &#34;property&#34;: {             