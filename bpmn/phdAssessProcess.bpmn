    <bpmn:serviceTask id="Activity_Notify_With_PDF_ProgramDirector_And_Office" name="Notify the Prog Director + Office">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="notification" />
        <zeebe:taskHeaders>
          <zeebe:header key="subject" value="Annual report of {{ phdStudentName }} (sciper {{ phdStudentSciper }}), whose general appraisal of the progress = {{ generalAppraisalOfTheProgress }}: PDF copy" />
          <zeebe:header key="message" value="&#60;p&#62;Dear {{ doctoralProgramName }} doctoral program Director,&#60;/p&#62;  &#60;p&#62;Thank you for having checked the annual report.&#60;/p&#62;  &#60;p&#62;You will find attached a PDF copy of the annual report of the doctoral candidate {{ phdStudentName }} (sciper {{ phdStudentSciper }}), whose general appraisal of the progress was rated as “{{ generalAppraisalOfTheProgress }}”. A copy is also saved on IS-Academia, in the GED file of the doctoral candidate, while the submission date and the general appraisal of the progress are saved in their “GPS”. Please note that the information is not stored in the online tool.&#60;/p&#62;  &#60;p&#62;Yours faithfully,&#60;/p&#62;  &#60;p&#62;EPFL Doctoral School&#60;/p&#62;" />
          <zeebe:header key="pdfName" value="phd_annual_report" />
        </zeebe:taskHeaders>
        <zeebe:ioMapping>
          <zeebe:input source="= programDirectorEmail" target="to" />
          <zeebe:input source="= doctoralProgramEmail" target="cc" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_181w5as</bpmn:incoming>
      <bpmn:outgoing>Flow_19hd8zh</bpmn:outgoing>
    </bpmn:serviceTask>
