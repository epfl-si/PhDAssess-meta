d)&#34;,               &#34;value&#34;: &#34;1st year&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;2nd year&#34;,               &#34;value&#34;: &#34;2nd year&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;3rd year&#34;,               &#34;value&#34;: &#34;3rd year&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;4th year&#34;,               &#34;value&#34;: &#34;4th year&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;5th year&#34;,               &#34;value&#34;: &#34;5th year&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;6th year&#34;,               &#34;value&#34;: &#34;6th year&#34;,               &#34;shortcut&#34;: &#34;&#34;             }           ],           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;year&#34;,           &#34;type&#34;: &#34;radio&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;title&#34;: &#34;&#60;b&#62;Section 1: Basic information&#60;/b&#62;&#34;,       &#34;collapsible&#34;: false,       &#34;key&#34;: &#34;panel&#34;,       &#34;type&#34;: &#34;panel&#34;,       &#34;label&#34;: &#34;Section A: Basic information&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false,       &#34;components&#34;: [         {           &#34;label&#34;: &#34;Name of doctoral candidate:&#34;,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;customDefaultValue&#34;: &#34;value = Meteor.user().tequila.displayname;&#34;,           &#34;key&#34;: &#34;phdStudentName&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Tentative thesis title:&#34;,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;tentativeThesisTitle&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director:&#34;,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisDirectorName&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director:&#34;,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisCoDirectorName&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Date of candidacy exam:&#34;,           &#34;widget&#34;: {             &#34;type&#34;: &#34;calendar&#34;,             &#34;altInput&#34;: true,             &#34;allowInput&#34;: true,             &#34;clickOpens&#34;: true,             &#34;enableDate&#34;: true,             &#34;enableTime&#34;: false,             &#34;mode&#34;: &#34;single&#34;,             &#34;noCalendar&#34;: false,             &#34;format&#34;: &#34;dd.MM.yyyy&#34;,             &#34;dateFormat&#34;: &#34;yyyy-MM-dd&#34;,             &#34;useLocaleSettings&#34;: false,             &#34;hourIncrement&#34;: 1,             &#34;minuteIncrement&#34;: 5,             &#34;time_24hr&#34;: false,             &#34;saveAs&#34;: &#34;text&#34;,             &#34;locale&#34;: &#34;en&#34;,             &#34;displayInTimezone&#34;: &#34;viewer&#34;           },           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;dateOfCandidacyExam&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;TA activities completed:&#60;span style=&#39;color:red;&#39;&#62; *&#60;/span&#62;&#60;br&#62;&#60;i&#62;Estimated hours and/or courses completed, supervision of project, ...&#60;/i&#62;&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;taDutiesHoursAndOrCoursesCompleted&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Credits needed for this program:&#34;,           &#34;inputMask&#34;: &#34;99&#34;,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;persistent&#34;: false,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;creditsNeeded&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Number of credits obtained:&#34;,           &#34;mask&#34;: false,           &#34;tableView&#34;: true,           &#34;delimiter&#34;: false,           &#34;requireDecimal&#34;: false,           &#34;inputFormat&#34;: &#34;plain&#34;,           &#34;truncateMultipleSpaces&#34;: false,           &#34;calculateServer&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;totalCreditsObtained&#34;,           &#34;type&#34;: &#34;number&#34;,           &#34;inputType&#34;: &#34;number&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Number of credits planned for the coming year (next 365 days):&#34;,           &#34;mask&#34;: false,           &#34;tableView&#34;: true,           &#34;delimiter&#34;: false,           &#34;requireDecimal&#34;: false,           &#34;inputFormat&#34;: &#34;plain&#34;,           &#34;truncateMultipleSpaces&#34;: false,           &#34;calculateServer&#34;: true,           &#34;key&#34;: &#34;totalCreditsPlanned&#34;,           &#34;type&#34;: &#34;number&#34;,           &#34;inputType&#34;: &#34;number&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;title&#34;: &#34;&#60;b&#62;Section 2: Research progress&#60;/b&#62;&#34;,       &#34;collapsible&#34;: false,       &#34;key&#34;: &#34;sectionBResearchProgress1&#34;,       &#34;conditional&#34;: {         &#34;show&#34;: false,         &#34;when&#34;: &#34;year&#34;,         &#34;eq&#34;: &#34;1st year&#34;       },       &#34;type&#34;: &#34;panel&#34;,       &#34;label&#34;: &#34;Section B: Research progress&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false,       &#34;components&#34;: [         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;To be completed first by the doctoral candidate and then discussed with the thesis director (and co-director if applicable) during a meeting (collaborative review).  Future objectives are meant to provide guidance in the thesis and should thus be as realistic as possible. Achieved and planned objectives should be discussed and agreed upon by the doctoral candidate and thesis director(s) before approving the Annual Report. &#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;1. Overall goals of the thesis &#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;goalsOfTheThesis&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;2. Research progress during the past year&#60;span style=&#39;color:red;&#39;&#62; *&#60;/span&#62;&#60;br&#62;&#60;i&#62;(i.e. describe which objectives are completed and elaborate if any difficulties have been encountered since the last annual report)&#60;/i&#62;&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;researchProgressDuringThePastYear&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;3. Have the objectives changed in the past year? If so, why and how? &#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;haveTheObjectivesChangedInThePastYearIfSoWhyAndHow&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;4. Research objectives for the coming year (next 365 days)&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;researchObjectivesForTheNextYear&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;5. Timeline of the planned work for the coming year (next 365 days)&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;timelineOfThePlannedWorkForTheNextYear&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;6. Scientific output&#60;span style=&#39;color:red;&#39;&#62; *&#60;/span&#62;&#60;br&#62;&#60;i&#62;(journal and conference papers, patents, software, etc. published, submitted or in preparation)&#60;/i&#62;&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;scientificOutput&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;7. Educational activities&#60;span style=&#39;color:red;&#39;&#62; *&#60;/span&#62;&#60;br&#62;&#60;i&#62;(i.e. Mention oral or poster presentations, outreach and any specific training)&#60;/i&#62;&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;educationalActivities&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;title&#34;: &#34;&#60;b&#62;Section 3: Progress assessment&#60;/b&#62;&#34;,       &#34;collapsible&#34;: false,       &#34;key&#34;: &#34;sectionCProgressAssessment1&#34;,       &#34;type&#34;: &#34;panel&#34;,       &#34;label&#34;: &#34;Section C: Progress assessment&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false,       &#34;components&#34;: [         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;To be completed independently by the doctoral candidate and thesis director (and co-director if applicable), then discussed and approved by both during a meeting (collaborative review). The goal is to have their respective perspective on the same topic, e.g. how the doctoral candidate views the supervision they receive and how the thesis director(s) views the supervision they give.&#60;br&#62;&#60;br&#62;The progress assessment is not confidential: each person’s comments initially remain hidden in order to provide all participants with the opportunity to express their view independently. When all parties have completed the self-assessment, comments are revealed to everyone so that they can prepare for the collaborative review. &#38;nbsp;&#60;/p&#62;\n&#60;p&#62;Aims on the doctoral candidate side:&#60;/p&#62;\n&#60;ul&#62;\n&#60;li&#62;to assess how the doctoral candidate views their own progress and skills&#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;to identify and communicate to the thesis director(s) in which areas the doctoral candidate seeks more input or help&#60;/li&#62;\n&#60;/ul&#62;\n&#60;p&#62;Aims on the thesis director (and co-director if applicable) side:&#60;/p&#62;\n&#60;ul&#62;\n&#60;li&#62;to ensure that the doctoral candidate knows how they are performing overall according to the thesis director’s expectations&#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;to communicate what the doctoral candidate’s strengths and weaknesses towards completing a PhD&#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;to communicate which areas need improvement&#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;to ensure that career issues are discussed at least once per year&#38;nbsp;&#60;/li&#62;\n&#60;/ul&#62;\n&#60;p&#62;The comments box should be used to briefly assess the progress and further elaborate any specific issues.&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html1&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;h4&#62;&#60;b&#62;Progress in the thesis and development as a researcher:&#60;/b&#62;&#60;/h4&#62;\n&#60;p&#62;Please comment on:&#60;/p&#62;\n&#60;ul&#62;\n&#60;li&#62;Doctoral candidate’s engagement in the thesis (satisfaction, motivation, commitment, initiative, independence, …);&#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;Thesis progress (quality of results, critical thinking, vision, timeline, organization, planning, …); &#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;Doctoral candidate’s scientific and career development (training, acquired skills – writing, presenting – teaching load, conferences and networking, after-PhD planning, …);&#60;/li&#62;\n&#60;/ul&#62; &#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html2&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Doctoral candidate comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;phdComment1&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisDirectorComment1&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisCoDirectorComment1&#34;,           &#34;logic&#34;: [             {               &#34;name&#34;: &#34;Comment1CoDirectorLogic&#34;,               &#34;trigger&#34;: {                 &#34;type&#34;: &#34;simple&#34;,                 &#34;simple&#34;: {                   &#34;show&#34;: true,                   &#34;when&#34;: &#34;thesisCoDirectorName&#34;,                   &#34;eq&#34;: &#34;&#34;                 }               },               &#34;actions&#34;: [                 {                   &#34;name&#34;: &#34;Comment1Action&#34;,                   &#34;type&#34;: &#34;property&#34;,                   &#34;property&#34;: {                     &#34;label&#34;: &#34;Hidden&#34;,                     &#34;value&#34;: &#34;hidden&#34;,                     &#34;type&#34;: &#34;boolean&#34;                   },                   &#34;state&#34;: true                 }               ]             }           ],           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;h4&#62;&#60;b&#62;Context of the thesis and support available:&#60;/b&#62;&#60;/h4&#62;\n&#60;p&#62;Please comment on:&#60;/p&#62;\n&#60;ul&#62;\n&#60;li&#62;Thesis director’s (and co-director if applicable) supervision (availability, coaching, support, resources, training and conferences, …);&#60;/li&#62;\n&#60;/ul&#62;\n&#60;ul&#62;\n&#60;li&#62;Laboratory environment (support from colleagues and own collegiality, open communication and atmosphere, balance in collaborative projects, resources availability, …)&#60;/li&#62;\n&#60;/ul&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html3&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Doctoral candidate comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;phdComment2&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisDirectorComment2&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisCoDirectorComment2&#34;,           &#34;logic&#34;: [             {               &#34;name&#34;: &#34;CommentCoDirectorLogic&#34;,               &#34;trigger&#34;: {                 &#34;type&#34;: &#34;simple&#34;,                 &#34;simple&#34;: {                   &#34;show&#34;: true,                   &#34;when&#34;: &#34;thesisCoDirectorName&#34;,                   &#34;eq&#34;: &#34;&#34;                 }               },               &#34;actions&#34;: [                 {                   &#34;name&#34;: &#34;CommentAction&#34;,                   &#34;type&#34;: &#34;property&#34;,                   &#34;property&#34;: {                     &#34;label&#34;: &#34;Hidden&#34;,                     &#34;value&#34;: &#34;hidden&#34;,                     &#34;type&#34;: &#34;boolean&#34;                   },                   &#34;state&#34;: true                 }               ]             }           ],           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;Any other specific points:&#60;br&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html7&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Doctoral candidate comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;phdComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisDirectorComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisCoDirectorComment6&#34;,           &#34;logic&#34;: [             {               &#34;name&#34;: &#34;CommentCoDirectorLogic&#34;,               &#34;trigger&#34;: {                 &#34;type&#34;: &#34;simple&#34;,                 &#34;simple&#34;: {                   &#34;show&#34;: true,                   &#34;when&#34;: &#34;thesisCoDirectorName&#34;,                   &#34;eq&#34;: &#34;&#34;                 }               },               &#34;actions&#34;: [                 {                   &#34;name&#34;: &#34;CommentAction&#34;,                   &#34;type&#34;: &#34;property&#34;,                   &#34;property&#34;: {                     &#34;label&#34;: &#34;Hidden&#34;,                     &#34;value&#34;: &#34;hidden&#34;,                     &#34;type&#34;: &#34;boolean&#34;                   },                   &#34;state&#34;: true                 }               ]             }           ],           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Common comment&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;commonComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;title&#34;: &#34;&#60;b&#62;Overall appraisal&#60;/b&#62;&#34;,       &#34;collapsible&#34;: false,       &#34;key&#34;: &#34;sectionDOverallAppraisalAndSignatures&#34;,       &#34;type&#34;: &#34;panel&#34;,       &#34;label&#34;: &#34;Section D: Overall appraisal and signatures&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false,       &#34;components&#34;: [         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;By validating the Annual Report (checkbox below), the doctoral candidate and the thesis director (and co-director if applicable) agree that the discussion regarding the achieved and planned objectives as well as the progress assessment satisfy both parties, who thus approve the current report.&#60;/p&#62;\n&#60;p&#62;&#60;strong&#62;Notes&#60;/strong&#62;&#60;/p&#62;\n&#60;p&#62;For the doctoral candidate: The thesis director(s) is responsible for guiding the doctoral candidate in their research and for coaching them to reach the scientific level of a PhD. The critical feedback of the thesis director(s) plays a key role in this process and also for the success of the doctoral candidate beyond. Validation of the Annual Report indicates that the doctoral candidate has discussed with the thesis director(s), understood the expectations for improvement in the areas identified, and agreed on the plan to reach these milestones and assess their progress.&#60;/p&#62;\n&#60;p&#62;For the thesis director(s): Validation of the Annual Report indicates that the thesis director(s) has discussed all the above points with the doctoral candidate, considered their feedback regarding any potential problem, explained their expectations for improvement, when appropriate, including mechanisms for intermediate progress assessments, and discussed a plan for timely publication of the results and for the thesis defence.&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;&#60;b&#62;General appraisal of the progress&#60;/b&#62;&#34;,           &#34;optionsLabelPosition&#34;: &#34;right&#34;,           &#34;inline&#34;: false,           &#34;tableView&#34;: false,           &#34;values&#34;: [             {               &#34;label&#34;: &#34;exceeds expectations&#34;,               &#34;value&#34;: &#34;exceeds expectations&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;meets expectations&#34;,               &#34;value&#34;: &#34;meets expectations&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;needs some improvement&#34;,               &#34;value&#34;: &#34;needs some improvement&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;unsatisfactory&#34;,               &#34;value&#34;: &#34;unsatisfactory&#34;,               &#34;shortcut&#34;: &#34;&#34;             }           ],           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;generalAppraisalOfTheProgress&#34;,           &#34;type&#34;: &#34;radio&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p style=\&#34;color:red;\&#34;&#62;In case the annual report is unsatisfactory, the program director is informed and will seek a meeting with the doctoral candidate and the thesis director(s), to set goals and appropriate timelines for completion.&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html8&#34;,           &#34;conditional&#34;: {             &#34;show&#34;: true,             &#34;when&#34;: &#34;generalAppraisalOfTheProgress&#34;,             &#34;eq&#34;: &#34;unsatisfactory&#34;           },           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Program Director notif&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;hidden&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;\nif (data.generalAppraisalOfTheProgress != \&#34;unsatisfactory\&#34; &#38;&#38; data.generalAppraisalOfTheProgress != \&#34;needs some improvement\&#34;) {\n  value = \&#34;\&#34; ;\n} else {\n  value = \&#34;notif\&#34;; \n}&#34;,           &#34;key&#34;: &#34;programDirectorNotif&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Needs Improvement&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;hidden&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;\nif (data.generalAppraisalOfTheProgress != \&#34;needs some improvement\&#34;) {\n  value = \&#34;\&#34; ;\n} else {\n  value = \&#34;true\&#34;; \n}&#34;,           &#34;key&#34;: &#34;needsImprovement&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Unsatisfactroy&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;hidden&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;\nif (data.generalAppraisalOfTheProgress != \&#34;unsatisfactory\&#34;) {\n  value = \&#34;\&#34; ;\n} else {\n  value = \&#34;true\&#34;; \n}&#34;,           &#34;key&#34;: &#34;unsatisfactroy&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;type&#34;: &#34;button&#34;,       &#34;label&#34;: &#34;Submit&#34;,       &#34;key&#34;: &#34;submit&#34;,       &#34;disableOnInvalid&#34;: true,       &#34;input&#34;: true,       &#34;tableView&#34;: false     },     {       &#34;label&#34;: &#34;Cancel&#34;,       &#34;action&#34;: &#34;event&#34;,       &#34;showValidations&#34;: false,       &#34;theme&#34;: &#34;secondary&#34;,       &#34;tableView&#34;: false,       &#34;key&#34;: &#34;cancel&#34;,       &#34;type&#34;: &#34;button&#34;,       &#34;input&#34;: true,       &#34;event&#34;: &#34;cancelClicked&#34;     }   ] }" />
