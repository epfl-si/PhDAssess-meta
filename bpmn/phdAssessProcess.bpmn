          <zeebe:header key="formIO" value="{   &#34;_id&#34;: &#34;6094ea003614a90bb3112cc5&#34;,   &#34;type&#34;: &#34;form&#34;,   &#34;tags&#34;: [],   &#34;owner&#34;: &#34;605a091e390b9fa6e626d355&#34;,   &#34;revisions&#34;: &#34;&#34;,   &#34;_vid&#34;: 0,   &#34;title&#34;: &#34;Annual Progress Report&#34;,   &#34;display&#34;: &#34;form&#34;,   &#34;access&#34;: [     {       &#34;roles&#34;: [         &#34;605a093a1964dcf6c669c8dd&#34;,         &#34;605a093a1964dc7db769c8de&#34;,         &#34;605a093a1964dcd89969c8df&#34;       ],       &#34;type&#34;: &#34;read_all&#34;     }   ],   &#34;submissionAccess&#34;: [],   &#34;controller&#34;: &#34;&#34;,   &#34;properties&#34;: {},   &#34;settings&#34;: {},   &#34;name&#34;: &#34;annualProgressReport&#34;,   &#34;path&#34;: &#34;annualprogressreport&#34;,   &#34;project&#34;: &#34;605a093a1964dca89369c8dc&#34;,   &#34;created&#34;: &#34;2021-05-07T07:19:28.829Z&#34;,   &#34;modified&#34;: &#34;2021-05-11T15:57:35.231Z&#34;,   &#34;machineName&#34;: &#34;mcvvtebnveuvysz:annualProgressReport&#34;,   &#34;components&#34;: [     {       &#34;label&#34;: &#34;HTML&#34;,       &#34;attrs&#34;: [         {           &#34;attr&#34;: &#34;&#34;,           &#34;value&#34;: &#34;&#34;         }       ],       &#34;content&#34;: &#34;&#60;a href=\&#34;https://www.epfl.ch/education/phd/annual_report/thesis_codirector_fills_annual_report\&#34; target=\&#34;_blank\&#34;&#62;&#60;img src=\&#34;https://www.epfl.ch/education/phd/wp-content/uploads/2023/11/manual.png\&#34; width=\&#34;50\&#34; &#62;&#60;/a&#62;&#34;,       &#34;refreshOnChange&#34;: false,       &#34;key&#34;: &#34;html8&#34;,       &#34;type&#34;: &#34;htmlelement&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false     },     {       &#34;title&#34;: &#34;&#60;b&#62;Annual Report&#60;/b&#62;&#34;,       &#34;collapsible&#34;: false,       &#34;disabled&#34;: true,       &#34;key&#34;: &#34;panel1&#34;,       &#34;type&#34;: &#34;panel&#34;,  