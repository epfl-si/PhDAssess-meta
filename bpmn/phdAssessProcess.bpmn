false,           &#34;key&#34;: &#34;html7&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Doctoral candidate comment hide&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;hidden&#34;: true,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;calculateValue&#34;: &#34;\n&#34;,           &#34;key&#34;: &#34;phdComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Doctoral candidate comment&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;calculateValue&#34;: &#34;value = data.phdComment6;&#34;,           &#34;key&#34;: &#34;phdComment66&#34;,           &#34;conditional&#34;: {             &#34;show&#34;: false,             &#34;when&#34;: &#34;phdComment6&#34;           },           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director comment hide&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;hidden&#34;: true,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisDirectorComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis director comment&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;value = data.thesisDirectorComment6;&#34;,           &#34;key&#34;: &#34;thesisDirectorComment7&#34;,           &#34;conditional&#34;: {             &#34;show&#34;: false,             &#34;when&#34;: &#34;thesisDirectorComment6&#34;           },           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director comment hide&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;hidden&#34;: true,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisCoDirectorComment6&#34;,           &#34;logic&#34;: [             {               &#34;name&#34;: &#34;CommentCoDirectorLogic&#34;,               &#34;trigger&#34;: {                 &#34;type&#34;: &#34;simple&#34;,                 &#34;simple&#34;: {                   &#34;show&#34;: true,                   &#34;when&#34;: &#34;thesisCoDirectorName&#34;,                   &#34;eq&#34;: &#34;&#34;                 }               },               &#34;actions&#34;: [                 {                   &#34;name&#34;: &#34;CommentAction&#34;,                   &#34;type&#34;: &#34;property&#34;,                   &#34;property&#34;: {                     &#34;label&#34;: &#34;Hidden&#34;,                     &#34;value&#34;: &#34;hidden&#34;,                     &#34;type&#34;: &#34;boolean&#34;                   },                   &#34;state&#34;: true                 }               ]             }           ],           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis co-director comment&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;calculateValue&#34;: &#34;value = data.thesisCoDirectorComment6;&#34;,           &#34;key&#34;: &#34;thesisCoDirectorComment7&#34;,           &#34;conditional&#34;: {             &#34;show&#34;: false,             &#34;when&#34;: &#34;thesisCoDirectorComment6&#34;           },           &#34;logic&#34;: [             {               &#34;name&#34;: &#34;CommentCoDirectorLogic&#34;,               &#34;trigger&#34;: {                 &#34;type&#34;: &#34;simple&#34;,                 &#34;simple&#34;: {                   &#34;show&#34;: true,                   &#34;when&#34;: &#34;thesisCoDirectorName&#34;,                   &#34;eq&#34;: &#34;&#34;                 }               },               &#34;actions&#34;: [                 {                   &#34;name&#34;: &#34;CommentAction&#34;,                   &#34;type&#34;: &#34;property&#34;,                   &#34;property&#34;: {                     &#34;label&#34;: &#34;Hidden&#34;,                     &#34;value&#34;: &#34;hidden&#34;,                     &#34;type&#34;: &#34;boolean&#34;                   },                   &#34;state&#34;: true                 }               ]             }           ],           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Common comment&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;commonComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;title&#34;: &#34;&#60;b&#62;Section 4: Overall appraisal&#60;/b&#62;&#34;,       &#34;collapsible&#34;: false,       &#34;key&#34;: &#34;sectionDOverallAppraisalAndSignatures&#34;,       &#34;type&#34;: &#34;panel&#34;,       &#34;label&#34;: &#34;Section D: Overall appraisal and signatures&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false,       &#34;components&#34;: [         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;&#60;strong&#62;For the doctoral candidate:&#60;/strong&#62; The thesis director(s) is responsible for guiding the doctoral candidate in their research and for coaching them to reach the scientific level of a PhD. The critical feedback of the thesis director(s) plays a key role in this process and also for the success of the doctoral candidate beyond. Submitting the collaborative review indicates that the doctoral candidate has discussed with the thesis director(s), understood the expectations for improvement in the areas identified, and agreed on the plan to reach these milestones and assess their progress.&#60;/p&#62;\n&#60;p&#62;&#60;strong&#62;For the thesis director(s):&#60;/strong&#62; Submitting the annual report indicates that the thesis director(s) has discussed all the above points with the doctoral candidate, considered their feedback regarding any potential problem, explained their expectations for improvement, when appropriate, including mechanisms for intermediate progress assessments, discussed a plan for timely publication of the results and for the thesis defence, and thus approves the current report.&#60;/p&#62;\n&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;&#60;b&#62;General appraisal of the progress&#60;/b&#62;&#34;,           &#34;optionsLabelPosition&#34;: &#34;right&#34;,           &#34;inline&#34;: false,           &#34;tableView&#34;: false,           &#34;values&#34;: [             {               &#34;label&#34;: &#34;exceeds expectations&#34;,               &#34;value&#34;: &#34;exceeds expectations&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;meets expectations&#34;,               &#34;value&#34;: &#34;meets expectations&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;needs some improvement&#34;,               &#34;value&#34;: &#34;needs some improvement&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;unsatisfactory&#34;,               &#34;value&#34;: &#34;unsatisfactory&#34;,               &#34;shortcut&#34;: &#34;&#34;             }           ],           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;generalAppraisalOfTheProgress&#34;,           &#34;type&#34;: &#34;radio&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p style=\&#34;color:red;\&#34;&#62;In case the annual report is unsatisfactory, the program director is informed and will seek a meeting with the doctoral candidate and the thesis director(s), to set goals and appropriate timelines for completion.&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html8&#34;,           &#34;conditional&#34;: {             &#34;show&#34;: true,             &#34;when&#34;: &#34;generalAppraisalOfTheProgress&#34;,             &#34;eq&#34;: &#34;unsatisfactory&#34;           },           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;In case the annual report is rated as needs some improvement, the doctoral program director will be invited to check the report and indicate any comment they may have.&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html10&#34;,           &#34;conditional&#34;: {             &#34;show&#34;: true,             &#34;when&#34;: &#34;generalAppraisalOfTheProgress&#34;,             &#34;eq&#34;: &#34;needs some improvement&#34;           },           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Program Director notif&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;hidden&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;\nif (data.generalAppraisalOfTheProgress != \&#34;unsatisfactory\&#34; &#38;&#38; data.generalAppraisalOfTheProgress != \&#34;needs some improvement\&#34;) {\n  value = \&#34;\&#34; ;\n} else {\n  value = \&#34;notif\&#34;; \n}&#34;,           &#34;key&#34;: &#34;programDirectorNotif&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Needs Improvement&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;hidden&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;\nif (data.generalAppraisalOfTheProgress != \&#34;needs some improvement\&#34;) {\n  value = \&#34;\&#34; ;\n} else {\n  value = \&#34;true\&#34;; \n}&#34;,           &#34;key&#34;: &#34;needsImprovement&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Unsatisfactory&#34;,           &#34;applyMaskOn&#34;: &#34;change&#34;,           &#34;hidden&#34;: true,           &#34;tableView&#34;: true,           &#34;clearOnHide&#34;: false,           &#34;calculateValue&#34;: &#34;\nif (data.generalAppraisalOfTheProgress != \&#34;unsatisfactory\&#34;) {\n  value = \&#34;\&#34; ;\n} else {\n  value = \&#34;true\&#34;; \n}&#34;,           &#34;key&#34;: &#34;unsatisfactory&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;type&#34;: &#34;button&#34;,       &#34;label&#34;: &#34;Submit&#34;,       &#34;key&#34;: &#34;submit&#34;,       &#34;disableOnInvalid&#34;: true,       &#34;input&#34;: true,       &#34;tableView&#34;: false     },     {       &#34;label&#34;: &#34;Cancel&#34;,       &#34;action&#34;: &#34;event&#34;,       &#34;showValidations&#34;: false,       &#34;theme&#34;: &#34;secondary&#34;,       &#34;tableView&#34;: false,       &#34;key&#34;: &#34;cancel&#34;,       &#34;type&#34;: &#34;button&#34;,       &#34;input&#34;: true,       &#34;event&#34;: &#34;cancelClicked&#34;     }   ] }" />
