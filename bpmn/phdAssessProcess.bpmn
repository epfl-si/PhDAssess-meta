clickOpens&#34;: true,             &#34;enableDate&#34;: true,             &#34;enableTime&#34;: false,             &#34;mode&#34;: &#34;single&#34;,             &#34;noCalendar&#34;: false,             &#34;format&#34;: &#34;dd.MM.yyyy&#34;,             &#34;dateFormat&#34;: &#34;yyyy-MM-dd&#34;,             &#34;useLocaleSettings&#34;: false,             &#34;hourIncrement&#34;: 1,             &#34;minuteIncrement&#34;: 5,             &#34;time_24hr&#34;: false,             &#34;saveAs&#34;: &#34;text&#34;,             &#34;locale&#34;: &#34;en&#34;,             &#34;displayInTimezone&#34;: &#34;viewer&#34;           },           &#34;tableView&#34;: true,           &#34;customDefaultValue&#34;: &#34;value = moment().format(&#39;DD.MM.YYYY&#39;);&#34;,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;programDirectorDate&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;I have checked the annual report&#34;,           &#34;tableView&#34;: false,           &#34;defaultValue&#34;: false,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;programDirectorCheckedAnnualReport&#34;,           &#34;type&#34;: &#34;checkbox&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Program director comment&#34;,           &#34;autoExpand&#34;: false,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;programDirectorComment&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;type&#34;: &#34;button&#34;,       &#34;label&#34;: &#34;Submit&#34;,       &#34;key&#34;: &#34;submit&#34;,       &#34;disableOnInvalid&#34;: true,       &#34;input&#34;: true,       &#34;tableView&#34;: false     },     {       &#34;label&#34;: &#34;Cancel&#34;,       &#34;a